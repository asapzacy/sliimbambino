{
  "presets": [
    ["@babel/preset-env", {
      "targets": { "safari": 10 },
      "modules": false,
      "debug": false
    }],
    "@babel/preset-react",
    "@babel/preset-stage-0",
    "@babel/flow"
  ],
  "plugins": [
    "@babel/plugin-syntax-decorators"
  ],
  "env": {
    "test": {
      "presets": [["es2015"], "react", "stage-0"]
    }
  }
}
